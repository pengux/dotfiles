Xft.dpi: 98
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

! Solarized color scheme for the X Window System
!
! http://ethanschoonover.com/solarized

